<!DOCTYPE html>
<html>
  <% include ./partials/head %>
  <body>
      <%=error%>
    <h1>obat</h1>
    <form action="/obats/add" method="post">
            Nama Obat:<br>
            <input type="text" name="namaObat"><br>
            Implikasi Obat:<br>
            <select name="implikasiObat">
              <option value="0">TIDAK ADA</option>
              <% dataObats.forEach(function(obat) {%>
              <option value="<%= obat.id %>"><%= obat.namaObat %></option>
              <% }) %>
            </select>
            <input type="submit" value="Submit">
          </form>
          <br>
          <a href="/obats">back</a>
  </body>
</html>
