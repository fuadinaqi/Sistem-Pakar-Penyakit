<!DOCTYPE html>
<html>
<% include ./partials/head %>

  <body>
    <% include ./partials/navigation %>
    <h1>obat</h1>
    <a href="/obats/add">add obat</a>
    <br>
    <br>
    <table class="table">
      <thead class="thead-dark">
        <tr>
          <th>no</th>
          <th>nama obat</th>
          <th>implikasi</th>
          <th>action</th>
        </tr>
      </thead>

      <%let count = 1%>
        <%obat.forEach(function(row){%>
          <tr>

            <td>
              <%=count%>
            </td>
            <td>
              <%=row.namaObat%>
            </td>
            <td>
              <%=row.dataValues.namaImplikasi%>
            </td>
            <td>
              <a href="/obats/edit/<%=row.id%>">edit obat</a> |
              <a href="/obats/delete/<%=row.id%>" onclick="return confirm (`delete?`);">delete</a>
            </td>

          </tr>
          <%count++%>
            <%})%>

    </table>



  </body>

</html>
